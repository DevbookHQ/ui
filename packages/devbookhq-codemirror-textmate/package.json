{
  "name": "@devbookhq/codemirror-textmate",
  "version": "0.0.3",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "LICENSE",
  "files": [
    "dist/**",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "prepublish": "node ./scripts/cleanse-pkg \"a,b,prepublish,postpublish\" \"@devbookhq/tsconfig,eslint-config-devbookhq\"",
    "build": "tsup src/index.ts --format esm --target es2020 --dts --external react --sourcemap --loader .wasm=dataurl",
    "dev": "tsup src/index.ts --format esm --dts --target es2020 --watch --external react --sourcemap --loader .wasm=dataurl",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@devbookhq/tsconfig": "workspace:*",
    "@types/react": "^18.0.25",
    "eslint-config-devbookhq": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@codemirror/language": "^6.3.1",
    "react": "^18.2.0",
    "vscode-languageserver-protocol": "^3.17.2",
    "vscode-oniguruma": "^1.6.2",
    "vscode-textmate": "^7.0.3"
  }
}