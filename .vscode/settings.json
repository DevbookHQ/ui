{
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "lib/**": true,
    ".next/**": true,
    ".vscode/**": true,
    ".git/**": true,
    "node_modules/**": true,
    "package-lock.json": true,
    "yarl.lock":true
  },
  "files.watcherExclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "lib/**": true,
    ".next/**": true,
    ".vscode/**": true,
    ".git/**": true,
    "node_modules/**": true,
    "package-lock.json": true,
    "yarl.lock":true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "css.validate": false,
  "tailwindCSS.validate": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ]
}